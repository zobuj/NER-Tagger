{
  "manifest_version": 2,
  "name": "Named Entity Extractor",
  "version": "1.0",
  "description": "Extracts all of the named entities from the paragraphs in a requested page",
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
